{
  "schemaVersion": 1,
  "id": "strongholdfinder",
  "version": "${version}",

  "name": "Stronghold Finder",
  "description": "This mod helps the player with locating the Stronghold using trigonometry. Try typing /logpos in chat while looking exactly at the Eye of Ender. Perform this action 2 times.",
  "authors": [
    "Goby56"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/strongholdfinder/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "com.goby56.strongholdfinder.StrongholdFinder"
    ]
  },
  "mixins": [
    "strongholdfinder.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "1.19.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
